{
  "name": "tool-name",
  "author": "wsafight",
  "version": "0.0.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "dts watch",
    "build": "dts build",
    "test": "dts test",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "keywords": [],
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/tool-name.esm.js",
  "size-limit": [
    {
      "path": "dist/tool-name.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/tool-name.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "8.1.1",
    "husky": "8.0.3",
    "size-limit": "8.1.1",
    "dts-cli": "1.6.3",
    "tslib": "2.4.1",
    "typescript": "4.9.4"
  }
}
